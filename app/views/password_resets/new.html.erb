<% provide(:title, "Forgot Password") %>


    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
      <%= f.label :email, class: 'primary center f4 pt5' %>
      <%= f.email_field :email, class: 'w-100 f4 center tc' %>

      <%= f.submit "Submit", class: 'w-90 mt4 mb2 center f4'%>
    <% end %>
